generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Data {
  Id      Int    @id @default(autoincrement())
  Name    String
  Email   String
  Status  Status
  Message String @default(" ")
}

enum Status {
  Hindu
  Christian
  HinduChristian
  No
}
